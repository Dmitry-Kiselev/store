[program:{{ application_name }}]
command = /home/user/Projects/{{ application_name }}/{{ application_name }}/deploy/gunicorn_start.sh
user = user
stdout_logfile = /home/user/Projects/{{ application_name }}/logs/gunicorn_supervisor.log
redirect_stderr = true
environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8